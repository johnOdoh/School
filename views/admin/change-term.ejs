<%- include('./header.ejs') %>
    <!-- /. NAV SIDE  -->
    <div id="page-wrapper">
        <div id="page-inner">
            <div class="row">
                <div class="col-md-12">
                    <h2>Change Term</h2>
                </div>
            </div>
            <!-- /. ROW  -->
            <hr />
            <div class="row">
                <form action="/admin/changeTerm" method="POST">
                    <% if(errMessage.length !== 0) { %>
                        <h5 class="text-center text-danger">
                            <%= errMessage %>
                        </h5>
                        <% } if(successMessage.length !== 0) { %>
                            <h5 class="text-center text-success">
                                <%= successMessage %>
                            </h5>
                            <% } %>
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-md-6 col-xs-12" style="padding-right: 20px;">
                                            <div class="form-group">
                                                <h5><b>Term</b></h5>
                                                <select name="term" class="form-control">
                                                    <option>select term</option>
                                                    <option value="1st">1st</option>
                                                    <option value="2nd">2nd</option>
                                                    <option value="3rd">3rd</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-xs-12">
                                            <button type="submit" class="btn btn-danger btn-radius btn-brd grd1">Change</button>
                                        </div>
                                    </div>
                                </div>
                </form>
            </div>
        </div>
    </div>
    <!-- /. PAGE INNER  -->
    </div>
    <!-- /. PAGE WRAPPER  -->
    </div>
    <!-- /. WRAPPER  -->
    <!-- JQUERY SCRIPTS -->
    <script src="/portal/js/jquery-1.10.2.js"></script>
    <!-- BOOTSTRAP SCRIPTS -->
    <script src="/portal/js/bootstrap.min.js"></script>
    <!-- METISMENU SCRIPTS -->
    <script src="/portal/js/jquery.metisMenu.js"></script>
    <!-- CUSTOM SCRIPTS -->
    <script src="/portal/js/custom.js"></script>


    </body>

    </html>